{
    "name": "wtolk/crud",
    "description": "crud generator for Laravel",
    "version": "1.0.1",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "wtolk.ru",
            "email": "web@wtolk.ru"
        }
    ],
    "autoload": {
        "psr-4": {
            "Wtolk\\Crud\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Wtolk\\Crud\\Providers\\CrudServiceProvider"
            ]
        }
    },
    "require": {
        "php": "^8.2",
        "illuminate/support": "^12.0",
        "illuminate/database": "^12.0",
        "illuminate/http": "^12.0",
        "wtolk/eloquent-query-filter": "*",
        "machy8/xhtml-formatter": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.0",
        "orchestra/testbench": "^9.0"
    },
    "autoload-dev": {
        "psr-4": {
            "Wtolk\\Crud\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-install-cmd": [
            "Wtolk\\Crud\\Installer::addToViteConfig"
        ],
        "post-update-cmd": [
            "Wtolk\\Crud\\Installer::addToViteConfig"
        ]
    }
}
